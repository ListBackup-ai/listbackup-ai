{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "accountId": {
      "type": "string",
      "minLength": 1
    },
    "permissions": {
      "type": "object",
      "properties": {
        "canViewData": {
          "type": "boolean"
        },
        "canManageSources": {
          "type": "boolean"
        },
        "canRunBackups": {
          "type": "boolean"
        },
        "canViewReports": {
          "type": "boolean"
        },
        "canManageSettings": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    }
  },
  "required": ["accountId"],
  "additionalProperties": false
}